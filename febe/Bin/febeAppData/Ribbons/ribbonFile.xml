<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ribbon-layout version="23.08.2019">
  <basedir id="pdc-external">febeAppData\pdcVipIcons</basedir>
  <dictionary file="febeAppData\Ribbons\ribbon_Dictionary.xml" namespace="ribbon_general" />
  <section id="ribbon.section.file" label="File">
    <tooltip text="File Section" />
    <block>
      <row>
        <cmd changeEvent="true" cmdstyle="image-and-text-vertical" enabled="true" id="ribbon.cmd.open" label="Open" menu-label="Open File" run="default" visible="true">
          <icon icon-id="open" function-id="default"/>
          <tooltip text="File Open" />
          <acceleratorkey key="12" keymodifier="nothing" />
        </cmd>
		<separator/>
        <cmd changeEvent="true" cmdstyle="image-and-text-vertical" enabled="true" id="ribbon.cmd.new" label="New" menu-label="New File" run="default" visible="true">
          <icon icon-id="new" function-id="default"/>
          <tooltip text="File New" />
        </cmd>
        <cmd changeEvent="true" cmdstyle="image-and-text-vertical" enabled="true" id="ribbon.cmd.save" label="Save" menu-label="Save File" run="default" visible="true">
          <icon icon-id="save" function-id="default"/>
          <tooltip text="File Save" />
        </cmd>
      </row>
    </block>
  </section>
</ribbon-layout>